$('#dialog').html('<%= escape_javascript(render :partial => 'price') %>');
$('#dialog').dialog({ height: 500,width: 800, modal: true });

var avg_rating = $("#avg_rating").attr('value');
$('#star').raty({ readOnly: true,score: avg_rating,number: 5 });
$('#none_star').raty({ readOnly: true,score: 0,number: 5 });
var length = $("div[class*='separate_star_rating']").length;
for ( var i = 0; i < length; i++ ){
	var rating = $("#rating_" + i).attr('value');
	$("#star_" + i).raty({ readOnly: true, score: rating, number: 5 });
}